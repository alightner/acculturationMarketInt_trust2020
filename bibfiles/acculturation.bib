@article{petersErgodicityProblemEconomics2019,
  title = {The Ergodicity Problem in Economics},
  author = {Peters, Ole},
  year = {2019},
  month = dec,
  volume = {15},
  pages = {1216--1221},
  publisher = {{Nature Publishing Group}},
  issn = {1745-2481},
  doi = {10.1038/s41567-019-0732-0},
  abstract = {The ergodic hypothesis is a key analytical device of equilibrium statistical mechanics. It underlies the assumption that the time average and the expectation value of an observable are the same. Where it is valid, dynamical descriptions can often be replaced with much simpler probabilistic ones \textemdash{} time is essentially eliminated from the models. The conditions for validity are restrictive, even more so for non-equilibrium systems. Economics typically deals with systems far from equilibrium \textemdash{} specifically with models of growth. It may therefore come as a surprise to learn that the prevailing formulations of economic theory \textemdash{} expected utility theory and its descendants \textemdash{} make an indiscriminate assumption of ergodicity. This is largely because foundational concepts to do with risk and randomness originated in seventeenth-century economics, predating by some 200 years the concept of ergodicity, which arose in nineteenth-century physics. In this Perspective, I argue that by carefully addressing the question of ergodicity, many puzzles besetting the current economic formalism are resolved in a natural and empirically testable way.},
  copyright = {2019 Springer Nature Limited},
  file = {/Users/aaronlightner/Zotero/storage/2WP83HEM/Peters - 2019 - The ergodicity problem in economics.pdf;/Users/aaronlightner/Zotero/storage/BYZKMK52/s41567-019-0732-0.html},
  journal = {Nature Physics},
  language = {en},
  number = {12}
}

