@article{kirchlerEffectFastSlow2017,
  title = {The Effect of Fast and Slow Decisions on Risk Taking},
  author = {Kirchler, Michael and Andersson, David and Bonn, Caroline and Johannesson, Magnus and S{\o}rensen, Erik {\O}. and Stefan, Matthias and Tingh{\"o}g, Gustav and V{\"a}stfj{\"a}ll, Daniel},
